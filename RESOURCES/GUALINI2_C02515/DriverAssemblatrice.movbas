		End If

'SCRITTURA ----------------------------------
		WriteR_204=CLng([Assemblatrice_In:LARGHEZZA_FINESTRA])
		WriteR_205=CLng([Assemblatrice_In:ALTEZZA_FINESTRA])
		WriteR_209=[Assemblatrice_In:APERTURA]

		If [Assemblatrice_In:STROBE_DATI_R2] Then
			WriteR_206=1
		Else
			WriteR_206=0
		End If

		If [Assemblatrice_In:START_ROTAZIONE] Then
			WriteR_220=1
		Else
			WriteR_220=0
		End If

		IsacCnc.writeR(204,WriteR_204)
		On Error GoTo Fine
		IsacCnc.writeR(205,WriteR_205)
		On Error GoTo Fine
		IsacCnc.writeR(206,WriteR_206)
		On Error GoTo Fine
