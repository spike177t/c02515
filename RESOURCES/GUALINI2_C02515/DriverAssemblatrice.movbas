Sub Main
	Dim showDebugPrint As Boolean

	'inizializzo
	Set IsacCnc = New CncCOMItfc.CnItfcCls
	[Assemblatrice_Stato:NonComunica] = True
	AssChkPingOk = "0   [ OK ]"

	NoPing:
	Wait 0.3
	If SocketsInitialize() Then
		'ping the IP by passing the address,
		'text to send, and the ECHO structure.
		success = Ping(("10.200.200.20"), ("TestPing"), ECHO)
	Else
		Ass_NonComunica1 = True
		GoTo NoPing
	End If

	'AssChkPing = GetStatusCode(success)
	AssChkPing = GetStatusCode(success)
	If (AssChkPing &lt;&gt; AssChkPingOk) Then
		Ass_NonComunica1 = True
		GoTo NoPing
	End If

	Wait 3
	'IsacCnc.machinename="CNC2K1"
	IsacCnc.machinename="10.200.200.20"
	IsacCnc.DataAType =CncAType_atShort
	Ass_NonComunica1 = False

Ciclo:
	While Not IsInStoppingMode

		'If Not Ass_On Then
		'If Not [Assemblatrice_Stato:Ass_ON] Then
		'	Ass_NonComunica1 = True
		'	GoTo Fine
		'End If

		If SocketsInitialize() Then
			'ping the IP by passing the address,
			'text to send, and the ECHO structure.
			success = Ping(("10.200.200.20"), ("TestPing"), ECHO)
		Else
			Ass_NonComunica1 = True
			GoTo Fine
		End If

		AssChkPing = GetStatusCode(success)
		If (AssChkPing = AssChkPingOk) Then
			Ass_NonComunica1 = False
		Else
			Ass_NonComunica1 = True
			GoTo Fine
		End If

		Err.Clear
		On Error Resume Next
		'AssChkRead=IsacCnc.ReadR(99) '-&gt; AssChkRead=IsacCnc.ReadR(99) sostituita con [Assemblatrice_In:CHK_READ]
		[Assemblatrice_Out:CHK_READ]=IsacCnc.ReadR(99)
		On Error GoTo Fine
		'If AssChkRead=AssChkWrite Then '-&gt; AssChkWrite=IsacCnc.ReadR(99) sostituita con [Assemblatrice_Out:CHK_WRITE]
		If [Assemblatrice_Out:CHK_READ]=[Assemblatrice_In:CHK_WRITE] Then
			If [Assemblatrice_In:CHK_WRITE] &gt;= 333 Then
	    		[Assemblatrice_In:CHK_WRITE]=0
			End If
			AssChkConta=0
    		[Assemblatrice_In:CHK_WRITE]=[Assemblatrice_In:CHK_WRITE]+1
		Else
			If AssChkConta &lt; 9 Then
				AssChkConta=AssChkConta+1
			End If
		End If
		IsacCnc.writeR(98,[Assemblatrice_In:CHK_WRITE])
		On Error GoTo Fine
		If AssChkConta &lt; 9 Then
			Ass_NonComunica2 = False
		Else
			Ass_NonComunica2 = True
			GoTo Fine
		End If


'LETTURA -------------------------------------
		If showDebugPrint Then
			Debug.Print("AssemblatriceStrartReadVar: " &amp; CStr(Now()))
		End If

		ReadR_215=IsacCnc.ReadR(215)
		On Error GoTo Fine
		ReadR_216=IsacCnc.ReadR(216)
		On Error GoTo Fine
		ReadR_207=IsacCnc.ReadR(207)
		On Error GoTo Fine
		ReadR_221=IsacCnc.ReadR(221)
		On Error GoTo Fine
		ReadR_219=IsacCnc.ReadR(219)
		On Error GoTo Fine
		ReadR_100=IsacCnc.ReadR(100)
		On Error GoTo Fine
		ReadR_101=IsacCnc.ReadR(101)
		On Error GoTo Fine
		ReadR_102=IsacCnc.ReadR(102)
		On Error GoTo Fine
		ReadR_103=IsacCnc.ReadR(103)
		On Error GoTo Fine
		ReadR_104=IsacCnc.ReadR(104)

		If showDebugPrint Then
			Debug.Print("Assemblatrice FineReadVar: " &amp; CStr(Now()))
