		End If
		On Error GoTo Fine

		[Assemblatrice_Out:LARGHEZZA_FINESTRA]=ReadR_215
		[Assemblatrice_Out:ALTEZZA_FINESTRA]=ReadR_216
		[Assemblatrice_Out:BACK_STROBE_DATI_R2]=ReadR_207
		[Assemblatrice_Out:FINE_ROTAZIONE]=ReadR_221
		[Assemblatrice_Out:APERTURA]=ReadR_219

		If ReadR_100 &lt;&gt; 0 Then
			[Assemblatrice_Out:AUTO] = True
		Else
			[Assemblatrice_Out:AUTO] = False
		End If

		If ReadR_101 &lt;&gt; 0 Then
			[Assemblatrice_Out:AZZERATA] = True
		Else
