Dim WriteR_205 As Single 'WriteR_205	Scrittura2
Dim WriteR_206 As Single 'WriteR_206	Scrittura3
Dim WriteR_220 As Single 'WriteR_220	Scrittura4
Dim WriteR_209 As Single 'WriteR_209	Scrittura5

Dim AssChkPingOk As String
Dim AssChkPing As String
Dim AssChkConta As Integer
Dim Ass_NonComunica1 As Boolean
Dim Ass_NonComunica2 As Boolean

Dim success As Long
