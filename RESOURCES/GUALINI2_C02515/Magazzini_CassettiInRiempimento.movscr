<ScriptCode StartSel="3340" SelLength="0" OutStatusBar="1" OutLog="1" OutPrinter="1">'#Reference #System.Data, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089, processorArchitecture=x86
'#Language "WWB.NET"

Option Explicit

Imports System
Imports System.IO
Imports System.Data.Odbc

Dim objGrid As GridWndCmdTarget
Dim MySqlConnectionString As String

Public Sub Click()
	Dim strQuery As String
	'Dim nRows As Integer
	Dim i As Integer
	Dim mag As Integer
	Dim cass As Integer
	Dim selected As Boolean
	Dim exQuery As String

	Dim retMsgbox  As VbMsgBoxResult
	Dim msg As String

	'var per verifica celle selezionate
	Dim nMinRow As Integer
	Dim nMinCol As Integer
	Dim nMaxRow As Integer
	Dim nMaxCol As Integer

	Dim idCass As Integer
	Dim idMag As Integer
	Dim idQrt As String
	Dim nomePz As String

	Dim rikValida As Boolean
	rikValida = False

	If [loc_ImpostazioniImpianto:MySqlConnectionString] = "" Then
		MySqlConnectionString = "DRIVER={MySQL ODBC 5.3 ANSI Driver};" &amp; _
	      "SERVER=localhost;" &amp; _
	      "DATABASE=gualini;" &amp; _
	      "UID=root;" &amp; _
	      "PASSWORD=co.mar;" &amp; _
	      "OPTION=3;"
	Else
		MySqlConnectionString = [loc_ImpostazioniImpianto:MySqlConnectionString]
	End If

	objGrid = GetSynopticObject.GetSubObject("gridWindow").GetObjectInterface

	'verifico la selezione
	objGrid.GetSelectedRange(nMinRow, nMinCol, nMaxRow, nMaxCol)
	If nMinRow &lt; 0 Then
		MsgBox("Selezionare prima un elemento dalla tabella." ,vbExclamation, "Richiesta cassetto in Estrattore.")
		objGrid = Nothing
		Exit Sub
	End If

	idMag 	= IIf(objGrid.CellText(nMinRow,1) &lt;&gt; "", CInt(objGrid.CellText(nMinRow,1)), 0)
	idCass 	= IIf(objGrid.CellText(nMinRow,2) &lt;&gt; "", CInt(objGrid.CellText(nMinRow,2)), 0)
	idQrt 	= objGrid.CellText(nMinRow,4)
	'nomePz	= objGrid.CellText(nMinRow,5)
	nomePz = "RikManDaMag1"

	msg = "Per chiamare il cassetto  necessario disabilitare lo scarico automatico di MAG "
	If idMag = 1 Then
		If [loc_ImpostazioniImpianto:M1_ABIL] = 1 Then
			 MsgBox(msg &amp; CStr(idMag), vbOkOnly + vbExclamation, "Richiesta cassetto in Estrattore 2.")
			 Exit Sub
		End If
	ElseIf idMag = 2 Then
		If [loc_ImpostazioniImpianto:M2_ABIL] = 1 Then
			 MsgBox(msg &amp; CStr(idMag), vbOkOnly + vbExclamation, "Richiesta cassetto in Estrattore 2.")
			 Exit Sub
		End If
	End If

	If (	(idMag = 1 And idCass &gt; 0 And StatoCicloScarico = 1000 And Not [MAG1_ES2_RIK:STROBE] And [MAG1_ES2_ACT:WAIT]) _
		Or 	(idMag = 2 And idCass &gt; 0 And StatoCicloScarico = 1000 And Not [MAG2_ES2_RIK:STROBE] And [MAG2_ES2_ACT:WAIT]) _
		) And StatoCicloScarico = 1000 _
		Then
		msg = "Hai richiesto il cassetto: " &amp; CStr(idCass) &amp; " del magazzino: " &amp; CStr(idMag) &amp; " in Estrattore 2." _
				&amp; vbCrLf &amp; " Sei sicuro?"
		retMsgbox = MsgBox(msg, vbYesNo, "Richiesta cassetto in Estrattore 2." )
		If retMsgbox = vbYes Then
			If idMag = 1 Then
				'faccio controllo ulteriore per verificare di non aver gi assegnato la missione al robot
				If [MAG1_ES2_ACT:ID_CASSETTO] &gt; 0 Then
					msg = "Attenzione  presente un cassetto nell'estrattore MAG1_ES2." &amp; vbCrLf _
						&amp; "Rispondi ANNULLA per annullare la richiesta." &amp; vbCrLf _
						&amp; "Rispondi OK per forzare la richiesta del cassetto." &amp; vbCrLf _
						&amp; vbCrLf &amp; "Attenzione rispondendo OK i dati dei pezzi non saranno eliminati, ma rimessi all'interno del magazzino." _
						&amp; vbCrLf &amp; "Se i pezzi sono stati rimossi rispondere ANNULLA per annullare e far rientrare il cassetto con procedura apposita da pagina MAGAZZINI/Richiedi Cassetto in Estrattore 2"

					retMsgbox = MsgBox(msg, vbOkCancel, "Annullare richiesta cassetto in Magazzino 1 Estrattore 2." )
					rikValida = IIf(retMsgbox = vbOK, True, False)
				Else
					rikValida = True
				End If

				If rikValida Then
					[MAG1_ES2_RIK:ID_CASSETTO] = idCass
					[MAG1_ES2_RIK:ID_QUARTINA] = idQrt
					[MAG1_ES2_RIK:NOME_PEZZO_UNICO] = nomePz
					[MAG1_ES2_RIK:STROBE] = True
					OpenSynoptic("Magazzini_RikE2",eOpenSynopticMode.enum_ops_opennormal)
					loc_BtnBarMagazzini_SogliaColore = 6
				End If

			ElseIf idMag = 2 Then
				'faccio controllo ulteriore per verificare di non aver gi assegnato la missione al robot
				If [MAG2_ES2_ACT:ID_CASSETTO] &gt; 0 Then
					msg = "Attenzione  presente un cassetto nell'estrattore MAG2_ES2." &amp; vbCrLf _
						&amp; "Rispondi ANNULLA per annullare la richiesta." &amp; vbCrLf _
						&amp; "Rispondi OK per forzare la richiesta del cassetto." &amp; vbCrLf _
						&amp; vbCrLf &amp; "Attenzione rispondendo OK i dati dei pezzi non saranno eliminati, ma rimessi all'interno del magazzino." _
						&amp; vbCrLf &amp; "Se i pezzi sono stati rimossi rispondere ANNULLA per annullare e far rientrare il cassetto con progedura apposita da pagina MAGAZZINI/Richiedi Cassetto in Estrattore 2"

					retMsgbox = MsgBox(msg, vbOkCancel, "Annullare richiesta cassetto in Magazzino 1 Estrattore 2." )
					rikValida = IIf(retMsgbox = vbOK, True, False)
				Else
					rikValida = True
				End If

				If rikValida Then
					[MAG2_ES2_RIK:ID_CASSETTO] = idCass
					[MAG2_ES2_RIK:ID_QUARTINA] = idQrt
					[MAG2_ES2_RIK:NOME_PEZZO_UNICO] = nomePz
					[MAG2_ES2_RIK:STROBE] = True
					OpenSynoptic("Magazzini_RikE2",eOpenSynopticMode.enum_ops_opennormal)
					loc_BtnBarMagazzini_SogliaColore = 6
				End If
			End If

		End If
	Else
		MsgBox("Impossibile chiamare cassetto perch Estrattore non Pronto o StatoCicloScarico &lt;&gt; 1000." ,vbExclamation, "Richiesta cassetto in Estrattore 2.")
	End If

	objGrid = Nothing

End Sub

Sub LogMsg(msg As String)
	Dim prefix As String
	prefix = "Magazzini_ContenutoMag1.synoptic -&gt; "
	Debug.Print prefix &amp; msg
End Sub
