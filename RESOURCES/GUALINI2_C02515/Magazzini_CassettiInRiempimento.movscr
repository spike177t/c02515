<ScriptCode StartSel="1240" SelLength="0" OutStatusBar="1" OutLog="1" OutPrinter="1">Dim objGrid As GridWndCmdTarget

Public Sub SymbolLoading()
	Dim strQuery As String

	Set objGrid = GetSynopticObject.GetSubObject("gridWindow").GetObjectInterface

	objGrid.AutoLayout = False
	
	objGrid.AddColumn("MAG")
	objGrid.AddColumn("CASS")
	objGrid.AddColumn("POS_CASS")
	objGrid.AddColumn("ID_QUARTINA")
	'objGrid.AddColumn("ID_PEZZO")
	'objGrid.AddColumn("SLOT_PEZZO")
	objGrid.AddColumn("NOME_COMMESSA")
	objGrid.AddColumn("PEZZI PRESENTI")
	objGrid.AddColumn("LastUpdate")
	'objGrid.AddColumn("MAG")

	strQuery = "SELECT " _
					&amp; " `cassetti_in_riempimento`.`ID_MAGAZZINO` AS MAG," _
					&amp; " `cassetti_in_riempimento`.`ID_CASSETTO` AS CASS," _
					&amp; " pos.DESCRIZIONE AS POSIZIONE_CASSETTO,  " _
					&amp; " `cassetti_in_riempimento`.`ID_QUARTINA`," _
					&amp; " `cassetti_in_riempimento`.`NOME_COMMESSA`," _
					&amp; " `cassetti_in_riempimento`.`PEZZI_PRESENTI`," _
					&amp; " `cassetti_in_riempimento`.`lastupdate`" _
					&amp; " FROM `gualini`.`cassetti_in_riempimento` " _
					&amp; " join  `gualini`.`posizione_cassetti` AS pos on  `cassetti_in_riempimento`.`POSIZIONE` =  pos.ID_POSIZIONE_CASSETTI  " _
					&amp; " where `cassetti_in_riempimento`.`ID_MAGAZZINO` = 1" _
					&amp; " or `cassetti_in_riempimento`.`ID_MAGAZZINO` = 2" _
					&amp; " Order by MAG, CASS" _
					&amp; ";"

	objGrid.Query = strQuery
	objGrid.Refresh
	objGrid.RecalcLayout

End Sub
</ScriptCode>
