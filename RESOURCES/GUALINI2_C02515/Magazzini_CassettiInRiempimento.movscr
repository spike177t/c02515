<ScriptCode StartSel="2284" SelLength="0" OutStatusBar="1" OutLog="1" OutPrinter="1">'#Reference #System.Data, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089, processorArchitecture=x86
'#Language "WWB.NET"

Option Explicit

Imports System
Imports System.IO
Imports System.Data.Odbc

Dim objGrid As GridWndCmdTarget
Dim MySqlConnectionString As String

Public Sub Click()
	Dim strQuery As String
	'Dim nRows As Integer
	Dim i As Integer
	Dim mag As Integer
	Dim cass As Integer
	Dim selected As Boolean
	Dim exQuery As String

	Dim retMsgbox  As VbMsgBoxResult
	Dim msg As String

	'var per verifica celle selezionate
	Dim nMinRow As Integer
	Dim nMinCol As Integer
	Dim nMaxRow As Integer
	Dim nMaxCol As Integer

	Dim idCass As Integer
	Dim idMag As Integer
	Dim idQrt As String
	Dim nomePz As String

	Dim rikValida As Boolean
	rikValida = False

	If [loc_ImpostazioniImpianto:MySqlConnectionString] = "" Then
		MySqlConnectionString = "DRIVER={MySQL ODBC 5.3 ANSI Driver};" &amp; _
	      "SERVER=localhost;" &amp; _
	      "DATABASE=gualini;" &amp; _
	      "UID=root;" &amp; _
	      "PASSWORD=co.mar;" &amp; _
	      "OPTION=3;"
	Else
		MySqlConnectionString = [loc_ImpostazioniImpianto:MySqlConnectionString]
	End If

	objGrid = GetSynopticObject.GetSubObject("gridWindow").GetObjectInterface

	'verifico la selezione
	objGrid.GetSelectedRange(nMinRow, nMinCol, nMaxRow, nMaxCol)
	If nMinRow &lt; 0 Then
		MsgBox("Selezionare prima un elemento dalla tabella." ,vbExclamation, "Richiesta cassetto in Estrattore.")
		objGrid = Nothing
		Exit Sub
	End If
	idMag 	= IIf(objGrid.CellText(nMinRow,1) &lt;&gt; "", CInt(objGrid.CellText(nMinRow,1)), 0)
	idCass 	= IIf(objGrid.CellText(nMinRow,2) &lt;&gt; "", CInt(objGrid.CellText(nMinRow,2)), 0)
	idQrt 	= objGrid.CellText(nMinRow,4)
	'nomePz	= objGrid.CellText(nMinRow,5)
	nomePz = "RikManDaMag1"

	If 	(	(idMag = 1 And idCass &gt; 0 And Not [R1_STATUS:IN_MISSIONE] And Not [MAG1_ES1_RIK:STROBE] And [MAG1_ES1_ACT:WAIT]) _
		Or 	(idMag = 2 And idCass &gt; 0 And Not [R1_STATUS:IN_MISSIONE] And Not [MAG2_ES1_RIK:STROBE] And [MAG2_ES1_ACT:WAIT]) _
		) And StatoCicloCarico = 1000 _
		Then
		msg = "Hai richiesto il cassetto: " &amp; CStr(idCass) &amp; " del magazzino: " &amp; CStr(idMag) &amp; " in Estrattore 1." _
				&amp; vbCrLf &amp; " Sei sicuro?"
		retMsgbox = MsgBox(msg, vbYesNo, "Richiesta cassetto in Estrattore 1." )
		If retMsgbox = vbYes Then
			If idMag = 1 Then
				If [MAG1_ES1_ACT:BUSY] Then
					msg = "Attenzione il cassetto risulta bloccato da Missione Robot non terminata." &amp; vbCrLf _
						&amp; "Rispondi SI per Annullare la richiesta." &amp; vbCrLf _
						&amp; "Rispondi NO per rimuovere il blocco e forzare la richiesta del cassetto." &amp; vbCrLf _
						&amp; vbCrLf &amp; "Attenzione rispondendo NO i dati dell'eventuale pezzo in pinza potrebbero essere persi."

					retMsgbox = MsgBox(msg, vbYesNo, "Annullare richiesta cassetto in Estrattore 1." )
					rikValida = IIf(retMsgbox = vbNo, True, False)
				Else
					rikValida = True
				End If
				If rikValida Then
					[MAG1_ES1_RIK:ID_CASSETTO] = idCass
					[MAG1_ES1_RIK:ID_QUARTINA] = idQrt
					[MAG1_ES1_RIK:NOME_PEZZO_UNICO] = nomePz

					'forzo valori in ACT
					[MAG1_ES1_ACT:DEPO] = True
					[MAG1_ES1_ACT:BUSY] = False

					[MAG1_ES1_RIK:STROBE] = True
				End If

			ElseIf idMag = 2 Then
				If [MAG2_ES1_ACT:BUSY] Then
					msg = "Attenzione il cassetto risulta bloccato da Missione Robot non terminata." &amp; vbCrLf _
						&amp; "Rispondi SI per Annullare la richiesta." &amp; vbCrLf _
						&amp; "Rispondi NO per rimuovere il blocco e forzare la richiesta del cassetto." &amp; vbCrLf _
						&amp; vbCrLf &amp; "Attenzione rispondendo NO i dati dell'eventuale pezzo in pinza potrebbero essere persi."
					
					retMsgbox = MsgBox(msg, vbYesNo, "Annullare richiesta cassetto in Estrattore 1." )
					rikValida = IIf(retMsgbox = vbNo, True, False)
				Else
					rikValida = True
				End If
				If rikValida Then
					[MAG2_ES1_RIK:ID_CASSETTO] = idCass
					[MAG2_ES1_RIK:ID_QUARTINA] = idQrt
					[MAG2_ES1_RIK:NOME_PEZZO_UNICO] = nomePz

					'forzo valori in ACT
					[MAG2_ES1_ACT:DEPO] = True
					[MAG2_ES1_ACT:BUSY] = False

					[MAG2_ES1_RIK:STROBE] = True
				End If
			End If
		End If
	Else
		MsgBox("Impossibile chiamare cassetto perch il Robot R1  In Missione o Estrattore non Pronto" _
				&amp; " o StatoCicloCarico &lt;&gt; 1000." ,vbExclamation, "Richiesta cassetto in Estrattore 1.")
	End If

	objGrid = Nothing

End Sub

Sub LogMsg(msg As String)
	Dim prefix As String
	prefix = "Magazzini_ContenutoMag1.synoptic -&gt; "
	Debug.Print prefix &amp; msg
End Sub
</ScriptCode>
